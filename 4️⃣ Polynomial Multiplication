#include <stdio.h>

int main() {
    int a[10], b[10], c[20] = {0};
    int n, m, i, j;

    printf("Enter degree of first polynomial: ");
    scanf("%d", &n);
    printf("Enter coefficients:\n");
    for(i = 0; i <= n; i++)
        scanf("%d", &a[i]);

    printf("Enter degree of second polynomial: ");
    scanf("%d", &m);
    printf("Enter coefficients:\n");
    for(i = 0; i <= m; i++)
        scanf("%d", &b[i]);

    for(i = 0; i <= n; i++)
        for(j = 0; j <= m; j++)
            c[i + j] += a[i] * b[j];

    printf("Result = ");
    for(i = n + m; i >= 0; i--)
        printf("%dx^%d ", c[i], i);

    return 0;
}
