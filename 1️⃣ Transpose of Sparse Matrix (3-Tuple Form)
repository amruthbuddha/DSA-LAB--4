#include <stdio.h>

int main() {
    int a[10][3], t[10][3], n, i;

    printf("Enter number of non-zero elements: ");
    scanf("%d", &n);

    printf("Enter row col value:\n");
    for(i = 0; i < n; i++)
        scanf("%d %d %d", &a[i][0], &a[i][1], &a[i][2]);

    for(i = 0; i < n; i++) {
        t[i][0] = a[i][1];
        t[i][1] = a[i][0];
        t[i][2] = a[i][2];
    }

    printf("Transpose Sparse Matrix:\n");
    for(i = 0; i < n; i++)
        printf("%d %d %d\n", t[i][0], t[i][1], t[i][2]);

    return 0;
}
